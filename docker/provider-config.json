{
  "logger" : {
    "level" : "info"
  },
  "port" : 9000,
  "ssl": {
    "enabled" : false,
    "port" : 9443,
    "cert" : "certs/cert.pem",
    "key" : "certs/key.pem"
  },
  "marklogic": {
    "connection": {
      "host":     "<marklogic host name>",
      "port":     <marklogic port>,
      "user":     "<user name>",
      "password": "<password>",
      "authType": "DIGEST"
    }
  },
  "auth" : {
    "enabled" : false,
    "plugin" : "auth-direct-file",
    "options" : {
      "secret" : "<secret for token generation>",
      "identityStore" : "auth/user-store.json",
      "tokenExpirationMinutes" : 60,
      "useHttp" : true
    }
  }
}
